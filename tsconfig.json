{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "target": "es6",
    "noEmit": false,
    "module": "esnext",
    "moduleResolution": "node",
    "noResolve": false,
    "emitDeclarationOnly": false,
    "declaration": false,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": false,
    "baseUrl": ".",
    "paths": {
      "@SketchWrapper": [
        "src/components/SketchWrapper.astro"
      ],
      "@SketchWrapperESM": [
        "src/components/SketchWrapperESM.astro"
      ],
      "@SketchLoader": [
        "src/components/SketchLoader.jsx"
        ],
      "@CodeSnippet": [
        "src/components/CodeSnippet.astro"
      ]
    },
    "lib": [
      "dom",
      "es6"
    ],
    "typeRoots": [
      "node_modules/@types",
      "src/types"
    ],
    "types": [
      "astro/client",
      "node"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": [
    "**/*.astro",
    "src/content/**/*.ts",
    "src/types/*.d.ts",
    "src/content/config.ts"
  ]
}